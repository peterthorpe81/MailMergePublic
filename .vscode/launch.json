    "version": "0.2.0",
    "configurations": [
        {
            "type": "blazorwasm",
            "name": "Launch and Debug Blazor WebAssembly Application",
            "request": "launch",            
            "browser": "edge",
            "url": "https://localhost:7269",
            "timeout": 60000,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
              },
            "dotNetConfig": {
              "logging": { "moduleLoad": false }
            }
        },
        {
          "type": "blazorwasm",
          "name": "Release",
          "request": "launch",            
          "browser": "edge",
          "url": "https://localhost:7269",
          "timeout": 60000,
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Release"
            },
          "dotNetConfig": {
            "logging": { "moduleLoad": false }
          }
      }
    ]
}